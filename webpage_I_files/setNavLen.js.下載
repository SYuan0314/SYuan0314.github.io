define(["getNode","langFilter"],function(l,m){return function(b,d,n){var a={navLen:99,text:"more...",chText:"\u66f4\u591a...",debug:!1};$.extend(a,d);if(0===a.navLen)return!1;b=$(b);d=l.getChildGroup(b);var f=parseInt(b.data("setnavlen"),10)||a.navLen,e=a.text;m&&(e=a.chText);d.each(function(p,g){$(g).find(".list-text.nav").each(function(){var c=$(g),h=c.find(".ct ul"),k=h.find("li");k.length>f&&(c=c.find(".hd a").attr("href"),k.slice(f).remove(),h.append('<li class="is-more"><span><a title="'+e+
'" href="'+c+'">'+e+"</a></span></li>"))})});a.debug&&(console.log("\u9810\u8a2d\u503c:",a),console.log("\u6a94\u6848 "+n+".js \u5df2\u9806\u5229\u57f7\u884c\u3002"))}});