define(["langFilter"],function(e){return function(b,f,g){var a={language:"en",todayButton:new Date,debug:!1};$.extend(a,f);e&&(a.language="zh-tw");b=$(b).find('[type="date"]');b.on("keydown",function(c){$(this);13===c.which&&c.preventDefault()});b.each(function(c,h){var d=$(this),k=d.attr("value");d.attr("type","text");d.val(k);$(h).datepicker(a)});a.debug&&(console.log("\u9810\u8a2d\u503c:",a),console.log("\u6a94\u6848 "+g+".js \u5df2\u9806\u5229\u57f7\u884c\u3002"))}});