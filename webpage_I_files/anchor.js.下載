define(function(){return function(c,e,d){var b={bindNode:"body",speed:300,debug:!1};$.extend(b,e);var f=$("html,body");c=$(c).find("a");c.on(d,function(){var a=$(this);a=$(a.attr("href"));var g=$(".is-fixed").outerHeight()||0;a.length||(a=$(b.bindNode));f.stop().animate({scrollTop:a.offset().top-g},b.speed)});c.on("click",function(a){a.preventDefault();$(this).trigger(d)});b.debug&&(console.log("\u9810\u8a2d\u503c:",b),console.log("\u6a94\u6848 "+d+".js \u5df2\u9806\u5229\u57f7\u884c\u3002"))}});