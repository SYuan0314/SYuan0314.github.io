define(function(){

	function main(env, opt, file){

		var $set = {
			debug: false
		}

        var $listGroup = $('.group-list.message'),
            $msg = $listGroup.find('.simple-text.message');

            $msg.each(function(){

                var $a = $(this).find('a')
                if( $a.find('i').length < 1){
                    $(this).addClass('no-time')
                }
            })

		if($set.debug) {
			console.log('預設值:', $set);
			console.log('檔案 '+ file +'.js 已順利執行。');
		}
	}
	
	return main;
}); 